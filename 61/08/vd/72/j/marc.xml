        <marc:datafield tag="260" ind1=" " ind2=" ">
            <marc:subfield code="a">[London] :</marc:subfield>
            <marc:subfield code="b">Sold by Ric. Chiswell in St. Pauls Churchyard &amp; by Tho. Basset in Fleet street,</marc:subfield>
            <marc:subfield code="c">[1626]</marc:subfield>
