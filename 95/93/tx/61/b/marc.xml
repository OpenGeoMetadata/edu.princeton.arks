        <marc:datafield tag="245" ind1="1" ind2="2">
            <marc:subfield code="a">A map of the state of Pennsylvania</marc:subfield>
            <marc:subfield code="h">[map] /</marc:subfield>
            <marc:subfield code="c">by Reading Howell MDCCXCII.</marc:subfield>
        </marc:datafield>
        <marc:datafield tag="255" ind1=" " ind2=" ">
