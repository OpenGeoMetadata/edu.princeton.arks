            <marc:subfield code="c">(W 73°40ʹ--W 73°35ʹ/N 41°02ʹ--N 40°58ʹ).</marc:subfield>
        </marc:datafield>
        <marc:datafield tag="260" ind1=" " ind2=" ">
            <marc:subfield code="a">[Washington, D.C.] :</marc:subfield>
            <marc:subfield code="b">U.S. Coast Survey Office,</marc:subfield>
