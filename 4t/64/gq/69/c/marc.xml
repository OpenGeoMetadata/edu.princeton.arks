        <marc:datafield tag="245" ind1="1" ind2="0">
            <marc:subfield code="a">Harbors of Captain's Island East and Captain's Island West</marc:subfield>
            <marc:subfield code="h">[map] /</marc:subfield>
            <marc:subfield code="c">from a trigonometrical survey under the direction of F.R. Hassler, superintendent of the Survey of the Coast of the United States ; triangulation by J. Ferguson ; topography by C.M. Eakin ; hydrography by the party under the command of Lieutenant G.S. Blake ; verified by A.A. Humphreys.</marc:subfield>
        </marc:datafield>
        <marc:datafield tag="246" ind1="3" ind2="0">
            <marc:subfield code="a">Captain's Island East and Captain's Island West</marc:subfield>
        </marc:datafield>
        <marc:datafield tag="255" ind1=" " ind2=" ">
