        <marc:datafield tag="245" ind1="1" ind2="2">
            <marc:subfield code="a">A new and exact map of Spain &amp; Portugal</marc:subfield>
            <marc:subfield code="h">[map] :</marc:subfield>
            <marc:subfield code="b">divided into its kingdoms and principalities &amp;c with ye principal roads and considerable improvements /</marc:subfield>
            <marc:subfield code="c">the whole rectify'd according to ye newest observations, by H. Moll Geographer.</marc:subfield>
        </marc:datafield>
        <marc:datafield tag="246" ind1="3" ind2="0">
